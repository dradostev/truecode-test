{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Routes": {
      "auth_service": {
        "ClusterId": "auth_cluster",
        "Match": { "Path": "/auth/{**catchall}" },
        "Transforms": [
          { "PathPattern": "/{**catchall}" }
        ]
      },
      "currencies_service": {
        "ClusterId": "currencies_cluster",
        "Match": { "Path": "/currencies/{**catchall}" },
        "Transforms": [
          { "PathPattern": "/{**catchall}" }
        ]
      }
    },
    "Clusters": {
      "auth_cluster": {
        "Destinations": {
          "auth_backend": { "Address": "http://localhost:5032" }
        }
      },
      "currencies_cluster": {
        "Destinations": {
          "currencies_backend": { "Address": "http://localhost:5090" }
        }
      }
    }
  }
}

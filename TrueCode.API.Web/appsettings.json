{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": [
      {
        "RouteId": "grpc_service1",
        "ClusterId": "grpc_cluster1",
        "Match": { "Path": "/service1/{**catchall}" },
        "Transforms": [
          { "PathPattern": "/{**catchall}" }
        ]
      },
      {
        "RouteId": "grpc_service2",
        "ClusterId": "grpc_cluster2",
        "Match": { "Path": "/service2/{**catchall}" },
        "Transforms": [
          { "PathPattern": "/{**catchall}" }
        ]
      }
    ],
    "Clusters": {
      "grpc_cluster1": {
        "Destinations": {
          "destination1": { "Address": "http://localhost:5001" }
        }
      },
      "grpc_cluster2": {
        "Destinations": {
          "destination2": { "Address": "http://localhost:5002" }
        }
      }
    }
  }
}
